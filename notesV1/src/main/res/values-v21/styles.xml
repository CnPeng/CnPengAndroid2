<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#739ef2</item>
        <item name="colorPrimaryDark">#739ef2</item>
        <item name="colorAccent">#739ef2</item>
        <item name="android:listDivider">@drawable/shape_bk_dla</item>
        <item name="android:colorForeground">@android:color/background_light</item>
        <item name="android:navigationBarColor">@android:color/background_dark</item>
    </style>
    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:listDivider">@drawable/shape_bk_dla</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>
</resources>